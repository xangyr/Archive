library(car)
model1 = aov(glucose.level ~ bact + dose + bact:dose, data = df)
Anova(model1, type = "III")
plot(model1, which = c(1,2))
modelnoInter = aov(glucose.level ~ bact + dose, data = df)
anova(modelnoInter)
library(lsmeans)
library(multcompView)
library(multcomp)
library(knitr)
